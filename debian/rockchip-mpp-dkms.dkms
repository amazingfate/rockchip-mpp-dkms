PACKAGE_NAME="rk_vcodec"
PACKAGE_VERSION="#MODULE_VERSION#"
CLEAN="make clean"
MAKE[0]="make -C $kernel_source_dir M=$dkms_tree/$PACKAGE_NAME/$PACKAGE_VERSION/build/src ROCKCHIP_MPP_SERVICE=m"

BUILT_MODULE_NAME[0]="rk_vcodec"
DEST_MODULE_NAME[0]="${BUILT_MODULE_NAME[0]}"
BUILT_MODULE_LOCATION[0]="src"
DEST_MODULE_LOCATION[0]="/updates/dkms"

AUTOINSTALL=yes
